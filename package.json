{
  "name": "ppm_api",
  "version": "1.0.0",
  "description": "Patient profile Management API",
  "main": "src/app.ts",
  "scripts": {
    "dev": "nodemon",
    "debug": "npx ts-node-dev --respawn --transpile-only -r tsconfig-paths/register --debug app.ts",
    "build": "ttsc -P ./tsconfig.json",
    "start": "node build/app.js",
    "db:pull": "prisma db pull && prisma generate",
    "db:migrate": "npx prisma generate && npx prisma migrate dev --create-only && npx prisma migrate dev && npx prisma generate",
    "test": "jest",
    "db:migrate:prod": "npx prisma generate && npx prisma migrate deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coding-mountain/xo-sports-backend.git"
  },
  "keywords": [
    "ppm",
    "cron",
    "jobs",
    "js",
    "javascript",
    "typescript",
    "node",
    "express"
  ],
  "author": "Sabin Khanal",
  "license": "ISC",
  "dependencies": {
    "@kyvg/vue3-notification": "^2.6.1",
    "@prisma/client": "4.6.1",
    "@types/express-serve-static-core": "^4.17.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.9",
    "argon2": "^0.30.2",
    "cors": "^2.8.5",
    "crossenv": "^0.0.2-security",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "event-dispatch": "^0.4.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "husky": "^8.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "nodemailer-mailjet-transport": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "require-all": "^3.0.0",
    "swagger-ui-express": "^4.6.0",
    "ts-node": "^10.9.1",
    "typedi": "^0.10.0",
    "winston": "^3.8.2",
    "zod": "^3.19.1"
  },
